  <mtr>
   <mtd><mrow/>
   </mtd>
   <mtd>
    <mrow><mspace width="6px" /><mspace width="6px" /><mspace width="6px" /><mo> max </mo><mo form='prefix' fence='true' stretchy='true' symmetric='true'>(</mo>

     <mrow><mn>0</mn><mo>,</mo><mn>1</mn><mo>-</mo><mi>y</mi>
      <msup>
       <mrow><mi>u</mi>
       </mrow>
